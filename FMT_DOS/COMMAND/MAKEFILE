COMMAND.OBJ : COMMAND.C UTIL.H DOSLIB.H DEF.H DOSCALL.H
	LCC -c COMMAND.C



UTIL.OBJ : UTIL.C UTIL.H DOSLIB.H DEF.H DOSCALL.H
	LCC -c UTIL.C



DOSLIB.OBJ : DOSLIB.C DOSLIB.H UTIL.H DEF.H DOSCALL.H
	LCC -c DOSLIB.C



DOSCALL.OBJ : DOSCALL.C DOSCALL.H UTIL.H DEF.H DOSCALL.H
	LCC -c DOSCALL.C



COMMAND.EXE : COMMAND.OBJ UTIL.OBJ DOSLIB.OBJ DOSCALL.OBJ
	LCC COMMAND.OBJ UTIL.OBJ DOSLIB.OBJ DOSCALL.OBJ
	REN COMMAND.EXE COMMAND.COM
